<h1> Student Form </h1>

<form action="<%=students_path%>" method="post">
<%=form_tag students_path do %>
<label> First Name: </label>

<%=text_field_tag :first_name, params[:first_name], id: 'first_name'%>

<label> Last Name: </label>

<%=text_field_tag :last_name, params[:last_name], id: 'last_name'%>

<%=submit_tag "Submit Student" %>
<% end %>
</form>

<%=params.inspect%>
